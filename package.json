{
  "name": "svelte-spectrum-components",
  "version": "0.1.0",
  "description": "Spectrum CSS components for Svelte",
  "keywords": [
    "svelte",
    "spectrum-css",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubermanu/svelte-spectrum-components.git"
  },
  "license": "MIT",
  "author": "Emmanuel Vodor <manu@paca.happy-dev.fr>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/assets",
    "!dist/histoire",
    "!dist/**/*.story.*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "vite build && npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check .",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "preview": "vite preview",
    "story:build": "histoire build",
    "story:dev": "histoire dev",
    "story:preview": "histoire preview",
    "test": "vitest"
  },
  "prettier": "@ubermanu/prettier-config",
  "dependencies": {
