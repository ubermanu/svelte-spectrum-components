    "@spectrum-css/closebutton": "^3.0.26",
    "@spectrum-css/colorarea": "^3.0.7",
    "@spectrum-css/colorhandle": "^3.0.4",
    "@spectrum-css/colorloupe": "^2.0.18",
    "@spectrum-css/combobox": "^1.0.6",
    "@spectrum-css/contextualhelp": "^1.0.4",
    "@spectrum-css/divider": "^2.0.25",
